{
  "name": "botui",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dbsetup:dev": "sls dynamodb migrate && sls dynamodb seed --seed=dev",
